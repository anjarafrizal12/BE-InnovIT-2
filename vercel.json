{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/upload",
        "methods": ["POST"],
        "dest": "/api/index.py"
      },
      {
        "src": "/get-data",
        "methods": ["POST"],
        "dest": "/api/index.py"
      }
    ]
  }